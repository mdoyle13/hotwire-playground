<%= form_with model: @event, scope: :event, url: wizard_path, method: :patch, data: { turbo: false } do |f| %>
  <%= f.label :starts_at %>
  <%= f.datetime_field :starts_at, value: @event.starts_at %>
  
  <%= f.label :ends_at %>
  <%= f.datetime_field :ends_at, value: @event.ends_at %>

  <%= f.submit "Next" %>
<% end %>