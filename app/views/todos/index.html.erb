<h1>Todos</h1>
<%= turbo_stream_from :todos %>

<%= link_to "all", todos_path, data: {turbo_frame: "todos", turbo_action: "advance" } %> | 
<%= link_to "incomplete", incomplete_todos_path, data: {turbo_frame: "todos", turbo_action: "advance"} %> | 
<%= link_to "complete", complete_todos_path, data: {turbo_frame: "todos", turbo_action: "advance"} %>
<hr>
<%= render "todos", todos: @todos %>
